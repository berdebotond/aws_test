*_override.tf.json

# Ignore CLI configuration files
.terraformrc
terraform.rc

# Ignore .env files which might contain sensitive data
.env
*.env

# Ignore AWS credentials file
.aws/credentials

# macOS specific files
.DS_Store

# Editor specific files
.vscode/
.idea/
*.swp
*.swo 